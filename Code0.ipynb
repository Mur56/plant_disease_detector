    elif plant_category == 'Corn':
        model = model_corn
    elif plant_category == 'Grape':
        model = model_grape
    elif plant_category == 'Peach':
        model = model_peach
    elif plant_category == 'Potato':
        model = model_potato
    elif plant_category == 'Strawberry':
        model = model_strawberry
    elif plant_category == 'Tomato':
        model = model_tomato
    else:
